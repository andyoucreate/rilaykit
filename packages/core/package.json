{
  "name": "@rilaykit/core",
  "version": "9.0.0",
  "description": "Core types, configurations, and utilities for the RilayKit form library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "forms",
    "typescript",
    "validation",
    "builder-pattern"
  ],
  "author": "AND YOU CREATE <contact@andyoucreate.com>",
  "license": "MIT",
  "homepage": "https://rilay.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyoucreate/rilay.git"
  },
  "bugs": {
    "url": "https://github.com/andyoucreate/rilay/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "react": "^18.3.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
